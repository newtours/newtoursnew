
{% if tours_data is not empty %}
    <p>Tours List</p>
<table id="tour-list-main" class="table table-striped table-bordered" style="width:100%">
    <thead>
    <tr>
        <th>Id</th>
        <th>Dates</th>
        <th>Prices</th>
        <th>Name</th>
        <th>Days</th>
        <th>Link</th>
        <th>Image</th>
    </tr>
    </thead>
    <tbody>
    {% for key, value in tours_data %}
    <tr>
        <td>{{ value['tour_rowid'] }}</td>
        <td>{{ value['tour_dates'] }}</td>
        <td>{{ value['tour_prices'] }}</td>
        <td>{{ value['tour_name'] | raw  }}</td>
        <td>{{ value['tour_days'] }}</td>
        <td>{{ value['tour_link'] }}</td>
        <td>{{ value['tour_image'] }}</td>
        </tr>
    {% endfor %}
    </tbody>
</table>
{% endif %}

{% if dates_data is not empty %}
    <p>Date List for {{ table_title['name'] }} {{ table_title['days'] }}</p>
    <table id="tour-list-main" class="table table-striped table-bordered" style="width:100%">
        <thead>
        <tr>
            <th>Old Tour Id</th>
            <th>Date</th>
            <th>Prefix</th>
            <th>Suffix</th>
            <th>Remark</th>
            <th>Generated</th>
        </tr>
        </thead>
        <tbody>
        {% for key, value in dates_data %}
            <tr>
                <td>{{ value['dates_tour_rowid'] }}</td>
                <td>{{ value['date_date'] }}</td>
                <td>{{ value['date_prefix'] }}</td>
                <td>{{ value['date_suffix'] | raw  }}</td>
                <td>{{ value['date_remark'] }}</td>
                <td>{{ value['date_generated'] }}</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
{% endif %}




